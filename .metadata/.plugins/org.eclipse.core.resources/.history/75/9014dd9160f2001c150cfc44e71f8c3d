#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1,num2;
	printf("enter two numbers");
	scanf("%d%d",&num1,&num2);
	if(num1>num2)
	{
		printf("%d i the greatest number"num1);
	}
	else{
		puts("%d is greatest number"num2);
	}
	return EXIT_SUCCESS;
}
