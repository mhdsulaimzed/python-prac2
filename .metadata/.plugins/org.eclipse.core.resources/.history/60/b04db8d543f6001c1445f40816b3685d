#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int limit;
	for(int i=1;i<=j;i++)
	{
		for(int j=0;j<i;j++)
		{
			printf("*");
		}
		printf("\n");
	}
	return EXIT_SUCCESS;
}
