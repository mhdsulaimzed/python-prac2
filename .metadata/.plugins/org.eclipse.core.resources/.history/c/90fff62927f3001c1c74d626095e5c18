#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float mark;
	printf("plese enter your mark");
	scanf("%f",&mark);
	if(mark>=90)
	{
		printf("A");
		}
	else if(mark>=80&&mark<=89)
	{
		printf("B");
	}


	/* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
