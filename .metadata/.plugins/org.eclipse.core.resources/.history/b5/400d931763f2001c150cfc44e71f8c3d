#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1,num2,num3;
	printf("enter 3 numbers");
	scanf("%d%d%d",&num1,&num2,num3);
	if(num1>num2)
	{
		if(num1>num3)
		{
		printf("%d i the greatest number",num1);
	}
	}
		if(num2>num1)
		{
			if(num2>num3)
			{
				printf("%d is the greatest number",num2);
			}
		}
	else{
		printf("%d is greatest number",num3);
	}

	}
	return EXIT_SUCCESS;
}
